	"scripts": {
		"db:migrate": "sequelize --config config/config.js db:migrate",
		"db:rollback": "sequelize --config config/config.js db:migrate:undo",
		"db:migration:create": "sequelize --config config/config.js migration:create",
		"db:clear": "babel-node ./scripts/db-clear",
		"db:ingest": "babel-node --max_old_space_size=10000 ./scripts/db-ingest",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"lint:staged": "lint-staged",
		"start": "NODE_ENV=development nodemon src --exec babel-node",
		"test": "jest --verbose",
		"test-ci": "yarn && jest --ci",
		"oldtest": "babel-tape-runner ./server/tests/**/*.test.js | faucet",
		"jsdoc": "jsdoc src scripts -r -c .jsdoc.json -d docs",
		"build": "babel src -d build",
		"execute": "yarn pm2 start build/index.js --name express",
		"build-and-execute": "yarn execute && yarn build"
	},
	"pre-commit": "lint:staged",
	"lint-staged": {
		"*.{md,sh,txt,xml,html,json}": [
			"editorconfig-tools fix",
			"git add"
		],
		"*.{js,jsx}": [
			"eslint --fix",
			"git add"
		]
	},
	"repository": {
		"type": "git",
		"url": "git@gitlab.archimedes.digital:archimedes/graphql-textserver.git"
